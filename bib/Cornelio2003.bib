@article{Cornelio2003,
abstract = {Karyotypes of 93 individuals belonging to 18 accessions of mandarins, mandarin hybrids and two related species were analysed with the fluorochromes CMA and DAPI, to identify marker chromosomes. The karyotypes revealed highly differentiated banding patterns and could be classified in four groups (I-IV) according to the presence/absence of chromosomes with three bands (type A) or with two bands (one proximal and one terminal, type B, or both terminal, type C). The accessions of group I exhibited the simplest and homozygous karyotypes (lacking chromosome types A, B and C), represented by 'Sunki' and 'Cleopatra'. Group II (lacking chromosome types A and B) included three accessions of Mediterranean mandarins and 'Cravo' mandarin, all of them with very similar and almost completely homozygous karyotypes. All other karyotypes of groups II and III (lacking chromosome type A) were heterozygous for one or more chromosome pairs and most of them seemed to be hybrid derivatives from non-mandarin accessions. Group IV (with chromosome types A and B) was represented only by two heterozygous hybrids ('Murcott' and 'King'). The karyotype of most hybrids agrees with one of the possible combinations resulting from chromosome types segregation from their putative ancestor karyotypes, but at least 'Orlando' seemed to be a more complex hybrid. Comparing with banding patterns of other Citrus species, those of group I and the Mediterranean mandarins (group II) are the best candidates to represent C. reticulaia (sensu Swingle) as a true species.},
address = {Departamento de Biologia, Universidade de Pernambuco, Recife, Brazil Departamento de Bot{\^{a}}nica, Universidade Federal de Pernambuco, Recife, Brazil EMBRAPA Mandioca e Fruticultura, Cruz das Almas, Brazil Departamento de Biologia, Inst. de Cie. Biol{\'{o}}gicas, U},
